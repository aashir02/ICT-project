    "from datetime import date\n",
    "ch=input(\"Do you want to know about today [Y/N]:\")\n",
    "if(ch.upper()=='Y'):\n",
    "    tdate=date.today()\n",
    "    print(\"Year\",\"Month\",\"Day\",sep='-')\n",
    "    print(tdate)\n",
    "else:\n",
    "    print(\"Exiting\")\n",
    "    exit()\n",
