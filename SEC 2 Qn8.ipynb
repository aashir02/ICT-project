    "num=int(input(\"Enter number = \"))\n",
    "def fact(x):\n",
    "    if(x==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return x*fact(x-1)\n",
    "result=fact(num)\n",
    "print(\"The factoruial is \",result)\n"
